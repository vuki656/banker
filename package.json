{
    "name": "banker",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "web:dev": "yarn workspace web dev",
        "web:start": "yarn workspace web start",
        "web:build": "yarn workspace web build",
        "lint": "yarn lint:source && yarn lint:config && yarn lint:spell",
        "lint:fix": "eslint './packages/**/*' --fix --quiet",
        "lint:source": "eslint './packages/**/*' --quiet --cache --cache-strategy='content' --cache-location='.eslintcache/'",
        "lint:config": "prettier --loglevel warn --check {*,**/*}.{json,yml,yaml}",
        "lint:spell": "cspell lint '**' --no-summary --no-progress"
    },
    "devDependencies": {
        "@rimac-technology/eslint-config": "^23.0.0",
        "@types/node": "16.11.33",
        "cspell": "^5.20.0",
        "eslint": "8.15.0",
        "prettier": "^2.6.2",
        "typescript": "4.6.4"
    },
    "packageManager": "yarn@3.2.0",
    "workspaces": [
        "packages/*"
    ]
}
