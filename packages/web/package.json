{
    "name": "@banker/web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "start": "next start",
        "build": "next build",
        "codegen": "graphql-codegen --config ./codegen.yaml",
        "production:start": "pm2 start yarn --interpreter bash --name client -- start",
        "production:kill": "pm2 stop client 2> /dev/null || true && pm2 delete client 2> /dev/null ||true",
        "production:restart": "yarn && yarn codegen && yarn build && yarn production:kill && yarn production:start"
    },
    "dependencies": {
        "@apollo/client": "3.6.9",
        "@hookform/resolvers": "^2.9.1",
        "@mantine/core": "^4.2.10",
        "@mantine/dates": "^4.2.10",
        "@mantine/dropzone": "^4.2.10",
        "@mantine/hooks": "^4.2.10",
        "@mantine/modals": "^4.2.10",
        "@mantine/next": "^4.2.10",
        "@mantine/notifications": "^4.2.10",
        "@react-hookz/web": "^14.2.3",
        "@tabler/icons": "^1.101.0",
        "cookies-next": "^2.0.5",
        "currency.js": "^2.0.4",
        "mobx": "^6.6.0",
        "mobx-react-lite": "^3.4.0",
        "next": "12.1.7-canary.45",
        "next-with-apollo": "^5.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.36.1",
        "recharts": "^2.1.10",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.12/xlsx-0.18.12.tgz",
        "zod": "^3.17.3"
    },
    "devDependencies": {
        "@graphql-codegen/fragment-matcher": "^3.3.1",
        "@graphql-codegen/introspection": "^2.2.1",
        "@graphql-codegen/near-operation-file-preset": "^2.4.1",
        "@graphql-codegen/typescript": "^2.7.3",
        "@graphql-codegen/typescript-operations": "2.5.3",
        "@graphql-codegen/typescript-react-apollo": "^3.3.3",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6"
    }
}
