{
    "name": "@banker/web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "start": "next start",
        "build": "next build",
        "codegen": "graphql-codegen --config ./codegen.yaml",
        "production:start": "pm2 start yarn --interpreter bash --name client -- start",
        "production:kill": "pm2 stop client 2> /dev/null || true && pm2 delete client 2> /dev/null ||true",
        "production:restart": "yarn && yarn codegen && yarn build && yarn production:kill && yarn production:start"
    },
    "dependencies": {
        "@apollo/client": "3.7.0",
        "@emotion/react": "^11.10.4",
        "@emotion/server": "^11.10.0",
        "@hookform/resolvers": "^2.9.8",
        "@mantine/core": "^5.4.2",
        "@mantine/dates": "^5.4.2",
        "@mantine/dropzone": "^5.4.2",
        "@mantine/hooks": "^5.4.2",
        "@mantine/modals": "^5.4.2",
        "@mantine/next": "^5.4.2",
        "@mantine/notifications": "^5.4.2",
        "@react-hookz/web": "^15.1.0",
        "@tabler/icons": "^1.101.0",
        "cookies-next": "^2.1.1",
        "currency.js": "^2.0.4",
        "mobx": "^6.6.2",
        "mobx-react-lite": "^3.4.0",
        "next": "12.3.1",
        "next-with-apollo": "^5.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.36.1",
        "recharts": "^2.1.10",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.12/xlsx-0.18.12.tgz",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@graphql-codegen/fragment-matcher": "^3.3.1",
