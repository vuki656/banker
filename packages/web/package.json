{
    "name": "@banker/web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "yarn codegen && next build",
        "codegen": "yarn graphql-codegen --config ./codegen.yaml",
        "dev": "next dev",
        "production:kill": "pm2 stop client 2> /dev/null || true && pm2 delete client 2> /dev/null ||true",
        "production:restart": "yarn && yarn codegen && yarn build && yarn production:kill && yarn production:start",
        "production:start": "pm2 start yarn --interpreter bash --name client -- start",
        "start": "next start"
    },
    "dependencies": {
        "@apollo/client": "3.7.10",
        "@emotion/react": "^11.10.6",
        "@emotion/server": "^11.10.0",
        "@hookform/resolvers": "^2.9.11",
        "@mantine/core": "^6.0.0",
        "@mantine/dates": "^6.0.0",
        "@mantine/dropzone": "^6.0.0",
        "@mantine/hooks": "^6.0.0",
        "@mantine/modals": "^6.0.0",
        "@mantine/next": "^6.0.0",
        "@mantine/notifications": "^6.0.0",
        "@nivo/core": "^0.80.0",
        "@nivo/line": "^0.80.0",
        "@react-hookz/web": "^22.0.0",
        "@tabler/icons": "^1.119.0",
        "cookies-next": "^2.1.1",
        "currency.js": "^2.0.4",
        "isomorphic-fetch": "^3.0.0",
        "js-cookie": "^3.0.1",
        "mobx": "^6.8.0",
        "mobx-react-lite": "^3.4.2",
        "next": "12.3.4",
        "next-cookies": "^2.0.3",
        "next-with-apollo": "^5.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.43.5",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.2/xlsx-0.19.2.tgz"
    },
    "devDependencies": {
        "@graphql-codegen/fragment-matcher": "^4.0.1",
        "@graphql-codegen/gql-tag-operations-preset": "^2.1.0",
        "@graphql-codegen/introspection": "^3.0.1",
        "@graphql-codegen/near-operation-file-preset": "^2.5.0",
        "@graphql-codegen/typescript": "^3.0.1",
        "@graphql-codegen/typescript-operations": "3.0.1",
        "@graphql-codegen/typescript-react-apollo": "^3.3.7",
        "@types/isomorphic-fetch": "^0.0.36",
        "@types/js-cookie": "^3.0.3",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11"
    }
